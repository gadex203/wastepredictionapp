{
  "expo": {
    "name": "wastepredictionapp",
    "slug": "wastepredictionapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      "expo-camera",
      "onnxruntime-react-native"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Bu uygulamanın geri dönüşüm yönlendirmesi için kamerayı kullanmasına izin verin.",
        "NSPhotoLibraryUsageDescription": "Geri dönüşüm yönlendirmesi için fotoğraflarınıza erişmesine izin verin."
      }
    },
    "android": {
      "package": "com.ahmed.wastepredictionapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
